<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {# <link rel="stylesheet" type="text/css" href="css/bootstrap.min.css"/> #}
        {# Run `composer require symfony/webpack-encore-bundle`
           and uncomment the following Encore helpers to start using Symfony UX #}
        {% block stylesheets %}
            {#{{ css('bootstrap.min.css') }}#}
            <link rel="stylesheet" type="text/css" href="css/bootstrap.css"/>
            <link rel="stylesheet" type="text/css" href="css/style.css"/>
        {% endblock %}

        {% block javascripts %}
            {#{{ encore_entry_script_tags('app') }}#}
        {% endblock %}
    </head>
    <body>
    <div class="col-md-12 col-sm-12">
	    <div class="form-group col-md-3 text-danger bg-dark">
            <div class="form-group col-md-9 ">
                <img src="img/P-accueil-e1527236718683.JPG" class="rounded"/><a href="{{path('index')}}">ACCEUIL</a>
            </div>
            <div class="form-group col-md-8 ">
                <img src="img/images.JPG" class="rounded"/><a href="{{path('box.index')}}">BOX</a>
            </div>
            <div class="form-group col-md-8 ">
                <img src="img/img_avatar3.png" class="rounded"/><a href="{{path('client.index')}}">CLIENT</a>
            </div>
            <div class="form-group col-md-8 ">
                <img src="img/000962694.JPG" class="rounded"/><a href="{{path('courant.index')}}">ELECTRICITE</a>
            </div>
            <div class="form-group col-md-8 ">
                <img src="img/construction-neuve.JPG" class="rounded"/><a href="{{path('logement.index')}}">LOGEMENT</a>
            </div>
            <div class="form-group col-md-8 ">
                <img src="img/art038_fig_couverture-1.JPG" class="rounded"/><a href="{{path('rel_circuit.index')}}">RELEVE</a>
            </div>
            <div class="form-group col-md-8 ">
                <img src="img/parametre-configuration.JPG" class="rounded"/><a href="{{path('reglage.index')}}">PARAMETTRE</a>
            </div>
            <div class="form-group col-md-8 ">
                <img src="img/cadenas.JPG" class="rounded"/><a href="{{path('protection.index')}}">PROTECTION</a>
            </div>
            <div class="form-group col-md-8 ">
                <img src="img/propos.JPG" class="rounded"/><a href="{{path('a_propos.index')}}">A PROPOS</a>
            </div>
        </div>
    </div>
        {% block body %}{% endblock %}
            <script type="text/javascript" src="js/jquery.js"></script>
            <script type="text/javascript" src="js/verif.js"></script>
            <script type="text/javascript" src="js/jquery-validate.js"></script> 
            <script type="text/javascript" src="js/app.js"></script> 
    </body>
</html>